    "@types/chai-spies": "^1.0.1",
    "@types/mocha": "^7.0.2",
    "@types/node": "^14.0.13",
    "@types/object-hash": "^1.3.3",
    "chai": "^4.1.2",
    "chai-spies": "^1.0.0",
